{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-formhelpers@2.3.0",
      "destination": "wwwroot/bootstrap-formhelpers/"
    },
    {
      "library": "font-awesome@5.11.2",
      "destination": "wwwroot/bootstrap-formhelpers/font-awesome/"
    },
    {
      "library": "ckeditor@4.12.1",
      "destination": "wwwroot/lib/ckeditor/"
    },
    {
      "library": "jquery-form-validator@2.3.79",
      "destination": "wwwroot/lib/jquery-form-validator/jquery-form-validator/",
      "files": [
        "jquery.form-validator.min.js",
        "security.js"
      ]
    },
    {
      "library": "spectrum@1.8.0",
      "destination": "wwwroot/vendor/spectrum/",
      "files": [
        "spectrum.min.js",
        "spectrum.min.css"
      ]
    }
  ]
}